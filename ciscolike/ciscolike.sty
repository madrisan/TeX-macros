\def\removeLastBox{\setbox0 = \lastbox}
% '\def\newline' (end-of-line def) can be customized, for instance:
%     \newcount\lineNum\lineNum = 0
%     \def\newline{%
%         \advance\lineNum by 1
%         \endgraf\noindent\leavevmode\llap{\sevenrm\the\lineNum.\ \ }}
%
% default is:
     \def\newline{\endgraf\noindent}
%\def\newline{\endgraf\noindent\leavevmode\llap{\quad}}
{\obeyspaces\gdef\tabtospaces{\ \ \ }}                            % 1 tab = 3 spaces
% command to be executed before and after each verbatim block
\newtoks\everyVerbatimBegin\everyVerbatimBegin = {\relax}
\newtoks\everyVerbatimEnd\everyVerbatimEnd = {\relax}
\newskip\verbatimBaselineskip\verbatimBaselineskip = \baselineskip
\let\verbatimFont = \tt                      % font to be used in the verbatim block

\bgroup
    \makeActive\^^M \makeActive\^^I %
    \gdef\setupVerbatim{%
        \makeActive\^^M \let^^M = \newline%                    % redefinition of ^^M
        \makeActive\^^I \let^^I = \tabtospaces%                    % tab into spaces
        \catcode`\@ = 0 \uncatCodeSpecials%             % no more special characters
        \parskip = 0pt \baselineskip = \verbatimBaselineskip%
        \obeyspaces\frenchspacing}
\egroup
\def\@{@}                                 % "@" can be used in \verbatim typing "@@"
\def\verbatim{\endgraf\bgroup\the\everyVerbatimBegin\setupVerbatim\verbatimFont}
\def\endverbatim{\removeLastBox\egroup{\the\everyVerbatimEnd}}

\def\verbindent{%
    \bgroup\everyVerbatimBegin = {%
        \medskip\advance\leftskip by4truemm%
        \hfuzz = \rightskipdimension%
        \verbatimBaselineskip = 0.7\baselineskip%
        \eightPoint\let\verbatimFont = \tt}%
    \verbatim}
\def\endverbindent{\everyVerbatimEnd = {\smallskip}\endverbatim\egroup}

\def\verbline{\bgroup\setupVerbatim\verbatimFont}
\def\endverbline{\egroup}
\def\nullhbox{\hbox{}}

